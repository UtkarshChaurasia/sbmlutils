        </math>
      </assignmentRule>
    </listOfRules>
    <listOfReactions>
      <reaction id="GLCIM" name="GLCIM (glc_ext &lt;-&gt; glc)" reversible="true" fast="false" compartment="Vbr">
        <listOfReactants>
          <speciesReference species="Aext_glc" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Abr_glc" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
            <apply>
              <divide/>
              <apply>
                <times/>
                <apply>
                  <divide/>
                  <ci> GLCIM_Vmax </ci>
                  <ci> GLCIM_Km </ci>
                </apply>
                <ci> Vbr </ci>
                <apply>
                  <minus/>
                  <ci> Cext_glc </ci>
                  <ci> Cbr_glc </ci>
                </apply>
              </apply>
              <apply>
                <plus/>
                <cn sbml:units="dimensionless" type="integer"> 1 </cn>
                <apply>
                  <divide/>
                  <ci> Cext_glc </ci>
                  <ci> GLCIM_Km </ci>
                </apply>
                <apply>
                  <divide/>
                  <ci> Cbr_glc </ci>
                  <ci> GLCIM_Km </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="LACIM" name="LACIM (lac_ext &lt;-&gt; lac)" reversible="true" fast="false" compartment="Vbr">
        <listOfReactants>
          <speciesReference species="Aext_lac" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Abr_lac" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
            <apply>
              <divide/>
              <apply>
                <times/>
                <apply>
                  <divide/>
                  <ci> LACIM_Vmax </ci>
                  <ci> LACIM_Km </ci>
                </apply>
                <ci> Vbr </ci>
                <apply>
                  <minus/>
                  <ci> Cext_lac </ci>
                  <ci> Cbr_lac </ci>
                </apply>
              </apply>
              <apply>
                <plus/>
                <cn sbml:units="dimensionless" type="integer"> 1 </cn>
                <apply>
                  <divide/>
                  <ci> Cext_lac </ci>
                  <ci> LACIM_Km </ci>
                </apply>
                <apply>
                  <divide/>
                  <ci> Cbr_lac </ci>
                  <ci> LACIM_Km </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="GLC2LAC" name="GLC2LAC (glc -&gt; 2 lac)" reversible="false" fast="false" compartment="Vbr">
        <listOfReactants>
          <speciesReference species="Abr_glc" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Abr_lac" stoichiometry="2" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> GLC2LAC_Vmax </ci>
              <ci> Vbr </ci>
              <apply>
                <divide/>
                <ci> Cbr_glc </ci>
                <apply>
                  <plus/>
                  <ci> Cbr_glc </ci>
                  <ci> GLC2LAC_Km </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
