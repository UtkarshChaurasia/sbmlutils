        <listOfReactants>
          <speciesReference species="glc_ext" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="glcRBC" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
            <apply>
              <divide/>
              <apply>
                <times/>
                <apply>
                  <divide/>
                  <ci> GLCIM_Vmax </ci>
                  <ci> GLCIM_Km </ci>
                </apply>
                <apply>
                  <minus/>
                  <apply>
                    <divide/>
                    <ci> glc_ext </ci>
                    <ci> Vpl </ci>
                  </apply>
                  <apply>
                    <divide/>
                    <ci> glcRBC </ci>
                    <ci> Vrbc </ci>
                  </apply>
                </apply>
              </apply>
              <apply>
                <plus/>
                <cn sbml:units="dimensionless" type="integer"> 1 </cn>
                <apply>
                  <divide/>
                  <apply>
                    <divide/>
                    <ci> glc_ext </ci>
                    <ci> Vpl </ci>
                  </apply>
                  <ci> GLCIM_Km </ci>
                </apply>
                <apply>
                  <divide/>
                  <apply>
                    <divide/>
                    <ci> glcRBC </ci>
                    <ci> Vrbc </ci>
                  </apply>
                  <ci> GLCIM_Km </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="LACEX" name="LACEX (lacRBC -&gt; Apl_lac)" reversible="false" fast="false" compartment="Vrbc">
        <listOfReactants>
          <speciesReference species="lacRBC" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="lac_ext" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
            <apply>
