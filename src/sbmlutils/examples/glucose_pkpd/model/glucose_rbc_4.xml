                <apply>
                  <divide/>
                  <apply>
                    <divide/>
                    <ci> lac_ext </ci>
                    <ci> Vpl </ci>
                  </apply>
                  <ci> LACEX_Km </ci>
                </apply>
                <apply>
                  <divide/>
                  <apply>
                    <divide/>
                    <ci> lacRBC </ci>
                    <ci> Vrbc </ci>
                  </apply>
                  <ci> LACEX_Km </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
