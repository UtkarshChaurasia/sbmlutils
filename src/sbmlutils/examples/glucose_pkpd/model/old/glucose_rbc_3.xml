    <listOfReactions>
      <reaction id="GLCIM" name="GLCIM (Apl_glc -&gt; glcRBC)" reversible="false" fast="false" compartment="Vrbc">
        <listOfReactants>
          <speciesReference species="Apl_glc" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="glcRBC" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
            <apply>
              <divide/>
              <apply>
                <times/>
                <apply>
                  <divide/>
                  <ci> GLCIM_Vmax </ci>
                  <ci> GLCIM_Km </ci>
                </apply>
                <apply>
                  <minus/>
                  <ci> CApl_glc </ci>
                  <ci> CglcRBC </ci>
                </apply>
              </apply>
              <apply>
                <plus/>
                <cn sbml:units="dimensionless" type="integer"> 1 </cn>
                <apply>
                  <divide/>
                  <ci> CApl_glc </ci>
                  <ci> GLCIM_Km </ci>
                </apply>
                <apply>
                  <divide/>
                  <ci> CglcRBC </ci>
                  <ci> GLCIM_Km </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="LACEX" name="LACEX (lacRBC -&gt; Apl_lac)" reversible="false" fast="false" compartment="Vrbc">
        <listOfReactants>
          <speciesReference species="lacRBC" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Apl_lac" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
            <apply>
              <divide/>
              <apply>
                <times/>
                <apply>
                  <divide/>
                  <ci> LACEX_Vmax </ci>
                  <ci> LACEX_Km </ci>
                </apply>
                <apply>
                  <minus/>
                  <ci> ClacRBC </ci>
                  <ci> CApl_lac </ci>
                </apply>
              </apply>
              <apply>
                <plus/>
                <cn sbml:units="dimensionless" type="integer"> 1 </cn>
                <apply>
                  <divide/>
                  <ci> CApl_lac </ci>
                  <ci> LACEX_Km </ci>
                </apply>
                <apply>
                  <divide/>
                  <ci> ClacRBC </ci>
                  <ci> LACEX_Km </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
      <reaction id="GLC2LAC" name="GLC2LAC (glcRBC -&gt; 2 lacRBC)" reversible="false" fast="false" compartment="Vrbc">
        <listOfReactants>
          <speciesReference species="glcRBC" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="lacRBC" stoichiometry="2" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> GLC2LAC_Vmax </ci>
              <apply>
                <divide/>
                <ci> CglcRBC </ci>
                <apply>
                  <plus/>
                  <ci> CglcRBC </ci>
                  <ci> GLC2LAC_Km </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
