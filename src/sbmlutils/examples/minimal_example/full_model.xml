    <listOfParameters>
      <parameter id="k1" value="0.1" units="litre_per_min" constant="true"/>
      <parameter id="J0_lb" name="lower flux bound J0" value="0" units="mmole_per_min" constant="true"/>
      <parameter id="J0_ub" name="upper flux bound J0" value="1000" units="mmole_per_min" constant="true"/>
    </listOfParameters>
    <listOfConstraints>
      <constraint>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <geq/>
            <ci> J0 </ci>
            <ci> J0_lb </ci>
          </apply>
        </math>
      </constraint>
      <constraint>
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <geq/>
            <ci> J0 </ci>
            <ci> J0_ub </ci>
          </apply>
        </math>
      </constraint>
    </listOfConstraints>
    <listOfReactions>
      <reaction metaid="meta_J0" id="J0" name="hexokinase" reversible="false" fast="false" compartment="cell" fbc:lowerFluxBound="J0_lb" fbc:upperFluxBound="J0_ub">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml">
Simplified hexokinase reaction ignoring ATP, ADP cofactors.Reaction is not mass and charge balanced.
</body>
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_J0">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/uniprot/P17710"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
        <listOfReactants>
          <speciesReference species="S1" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="S2" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> k1 </ci>
              <ci> S1 </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
