      <species metaid="meta_hex1" id="hex1" name="hexokinase protein" compartment="cyto" initialConcentration="1" substanceUnits="dimensionless" hasOnlySubstanceUnits="true" boundaryCondition="false" constant="false" conversionFactor="cf_units_per_mmole">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml" style="padding: 30px;background-color: white;font-family: Helvetica, arial, sans-serif;color: #333;line-height: 1.6;font-size: 14px;padding-top: 10px;padding-bottom: 10px;">
            <p style="margin-top: 0 !important;margin: 15px 0;">extent * conversionfactor = substanceUnit</p>
            <p style="margin-bottom: 0 !important;margin: 15px 0;">mmole * 1/mmole = dimensionless</p>
          </body>
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
