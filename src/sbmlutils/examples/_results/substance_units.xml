              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000176"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
        <listOfReactants>
          <speciesReference species="glc" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="glc6p" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="hex1"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> HEX1_Vmax </ci>
                <ci> hex1 </ci>
                <ci> glc </ci>
              </apply>
              <apply>
                <plus/>
                <ci> HEX1_Km_glc </ci>
                <ci> glc </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
