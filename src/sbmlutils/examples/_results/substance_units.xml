    <listOfUnitDefinitions>
      <unitDefinition id="kg">
        <listOfUnits>
          <unit kind="kilogram" exponent="1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="min">
        <listOfUnits>
          <unit kind="second" exponent="1" scale="0" multiplier="60"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="m">
        <listOfUnits>
          <unit kind="metre" exponent="1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="m2">
        <listOfUnits>
          <unit kind="metre" exponent="2" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="m3">
        <listOfUnits>
          <unit kind="metre" exponent="3" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mM">
        <listOfUnits>
          <unit kind="mole" exponent="1" scale="-3" multiplier="1"/>
          <unit kind="litre" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mmole">
        <listOfUnits>
          <unit kind="mole" exponent="1" scale="-3" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="per_mmole">
        <listOfUnits>
          <unit kind="mole" exponent="-1" scale="-3" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mmole_per_min">
        <listOfUnits>
          <unit kind="mole" exponent="1" scale="-3" multiplier="1"/>
          <unit kind="second" exponent="-1" scale="0" multiplier="60"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="per_min">
        <listOfUnits>
          <unit kind="second" exponent="-1" scale="0" multiplier="60"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfCompartments>
      <compartment id="cyto" name="cytosol" spatialDimensions="3" size="1" units="litre" constant="true"/>
    </listOfCompartments>
    <listOfSpecies>
      <species sboTerm="SBO:0000247" id="glc" name="D-glucose" compartment="cyto" initialConcentration="3" substanceUnits="mmole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species sboTerm="SBO:0000247" id="glc6p" name="D-glucose 6-phosphate" compartment="cyto" initialConcentration="0.5" substanceUnits="mmole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false"/>
      <species sboTerm="SBO:0000245" id="hex1" name="hexokinase protein" compartment="cyto" initialConcentration="1" substanceUnits="dimensionless" hasOnlySubstanceUnits="true" boundaryCondition="false" constant="false" conversionFactor="cf_units_per_mmole"/>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="cf_units_per_mmole" value="1" units="per_mmole" constant="true"/>
      <parameter sboTerm="SBO:0000186" id="HEX1SYNTHESIS_k" value="1" units="mmole_per_min" constant="true"/>
      <parameter sboTerm="SBO:0000186" id="HEX1_Vmax" value="1" units="mmole_per_min" constant="true"/>
      <parameter sboTerm="SBO:0000371" id="HEX1_Km_glc" value="0.1" units="mM" constant="true"/>
    </listOfParameters>
    <listOfReactions>
      <reaction sboTerm="SBO:0000176" id="HEX1SYNTHESIS" name="hexokinase synthesis" reversible="false" fast="false" compartment="cyto">
        <listOfProducts>
          <speciesReference species="hex1" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <ci> HEX1SYNTHESIS_k </ci>
          </math>
        </kineticLaw>
      </reaction>
      <reaction sboTerm="SBO:0000176" id="HEX1" name="hexokinase" reversible="false" fast="false" compartment="cyto">
        <listOfReactants>
          <speciesReference species="glc" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="glc6p" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <listOfModifiers>
          <modifierSpeciesReference species="hex1"/>
        </listOfModifiers>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <divide/>
              <apply>
                <times/>
                <ci> HEX1_Vmax </ci>
                <ci> hex1 </ci>
                <ci> glc </ci>
              </apply>
              <apply>
                <plus/>
                <ci> HEX1_Km_glc </ci>
                <ci> glc </ci>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
    <comp:listOfPorts>
      <comp:port metaid="kg_port" sboTerm="SBO:0000599" comp:unitRef="kg" comp:id="kg_port" comp:name="kg_port"/>
      <comp:port metaid="min_port" sboTerm="SBO:0000599" comp:unitRef="min" comp:id="min_port" comp:name="min_port"/>
      <comp:port metaid="m_port" sboTerm="SBO:0000599" comp:unitRef="m" comp:id="m_port" comp:name="m_port"/>
      <comp:port metaid="m2_port" sboTerm="SBO:0000599" comp:unitRef="m2" comp:id="m2_port" comp:name="m2_port"/>
      <comp:port metaid="m3_port" sboTerm="SBO:0000599" comp:unitRef="m3" comp:id="m3_port" comp:name="m3_port"/>
      <comp:port metaid="mM_port" sboTerm="SBO:0000599" comp:unitRef="mM" comp:id="mM_port" comp:name="mM_port"/>
      <comp:port metaid="mmole_port" sboTerm="SBO:0000599" comp:unitRef="mmole" comp:id="mmole_port" comp:name="mmole_port"/>
      <comp:port metaid="per_mmole_port" sboTerm="SBO:0000599" comp:unitRef="per_mmole" comp:id="per_mmole_port" comp:name="per_mmole_port"/>
      <comp:port metaid="mmole_per_min_port" sboTerm="SBO:0000599" comp:unitRef="mmole_per_min" comp:id="mmole_per_min_port" comp:name="mmole_per_min_port"/>
      <comp:port metaid="per_min_port" sboTerm="SBO:0000599" comp:unitRef="per_min" comp:id="per_min_port" comp:name="per_min_port"/>
    </comp:listOfPorts>
  </model>
