    </annotation>
    <listOfUnitDefinitions>
      <unitDefinition id="kg" name="kg">
        <listOfUnits>
          <unit kind="gram" exponent="1" scale="0" multiplier="1000"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="m" name="meter">
        <listOfUnits>
          <unit kind="metre" exponent="1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="m2" name="meter^2">
        <listOfUnits>
          <unit kind="metre" exponent="2" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="m3" name="meter^3">
        <listOfUnits>
          <unit kind="metre" exponent="3" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mmole" name="mmole">
        <listOfUnits>
          <unit kind="mole" exponent="1" scale="0" multiplier="0.001"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="mmole_per_s" name="mmole/s">
        <listOfUnits>
          <unit kind="mole" exponent="1" scale="0" multiplier="0.001"/>
          <unit kind="second" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="per_s" name="1/s">
        <listOfUnits>
          <unit kind="second" exponent="-1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
      <unitDefinition id="s" name="s">
        <listOfUnits>
          <unit kind="second" exponent="1" scale="0" multiplier="1"/>
        </listOfUnits>
      </unitDefinition>
    </listOfUnitDefinitions>
    <listOfCompartments>
      <compartment metaid="meta_Vc" id="Vc" name="cell compartment" spatialDimensions="3" size="1e-06" units="m3" constant="false">
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_Vc">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000290"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
      </compartment>
    </listOfCompartments>
    <listOfSpecies>
      <species metaid="meta_Aglc" id="Aglc" name="glucose" compartment="Vc" initialAmount="5" substanceUnits="mmole" hasOnlySubstanceUnits="true" boundaryCondition="false" constant="false">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml" style="font-family: Helvetica, arial, sans-serif;padding-bottom: 10px;color: #333;font-size: 14px;line-height: 1.6;background-color: white;padding: 30px;padding-top: 10px;">
            <p style="margin-bottom: 0 !important;margin: 15px 0;margin-top: 0 !important;">Species in amounts. The unit of the species is the <code style="margin: 0 2px;white-space: nowrap;border: 1px solid #eaeaea;background-color: #f8f8f8;padding: 0 5px;border-radius: 3px;">substanceUnit</code>,
i.e. [mmole].</p>
          </body>
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_Aglc">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000247"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
      </species>
      <species metaid="meta_Cglc6p" id="Cglc6p" name="glucose 6-phosphate" compartment="Vc" initialAmount="0" substanceUnits="mmole" hasOnlySubstanceUnits="false" boundaryCondition="false" constant="false">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml" style="font-size: 14px;padding-top: 10px;color: #333;font-family: Helvetica, arial, sans-serif;padding-bottom: 10px;padding: 30px;background-color: white;line-height: 1.6;">
            <p style="margin-bottom: 0 !important;margin: 15px 0;margin-top: 0 !important;">Species in concentration via the <code style="margin: 0 2px;white-space: nowrap;background-color: #f8f8f8;border: 1px solid #eaeaea;padding: 0 5px;border-radius: 3px;">hasOnlySubstanceUnit=False</code>.
The unit of the species is the
<code style="padding: 0 5px;background-color: #f8f8f8;border: 1px solid #eaeaea;margin: 0 2px;border-radius: 3px;white-space: nowrap;">substanceUnit/compartmentUnit</code>, i.e. [mmole/litre].</p>
          </body>
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_Cglc6p">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000247"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
      </species>
    </listOfSpecies>
    <listOfParameters>
      <parameter id="k1" value="1" units="per_s" constant="true"/>
    </listOfParameters>
    <listOfRules>
      <assignmentRule variable="Vc">
        <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
          <apply>
            <times/>
            <cn sbml:units="m3"> 2 </cn>
            <apply>
              <exp/>
              <apply>
                <divide/>
                <csymbol encoding="text" definitionURL="http://www.sbml.org/sbml/symbols/time"> time </csymbol>
                <cn sbml:units="s" type="integer"> 1 </cn>
              </apply>
            </apply>
          </apply>
        </math>
      </assignmentRule>
    </listOfRules>
    <listOfReactions>
      <reaction metaid="meta_R1" id="R1" reversible="false" fast="false" compartment="Vc">
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_R1">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000176"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
        <listOfReactants>
          <speciesReference species="Aglc" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="Cglc6p" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML">
            <apply>
              <times/>
              <ci> k1 </ci>
              <ci> Aglc </ci>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
  </model>
</sbml>
