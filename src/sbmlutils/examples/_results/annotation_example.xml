    <listOfReactions>
      <reaction metaid="meta_e__GLUT2_GAL" id="e__GLUT2_GAL" name="galactose transport [e__]" reversible="true" fast="false" compartment="pm">
        <notes>
          <body xmlns="http://www.w3.org/1999/xhtml"/>
        </notes>
        <annotation>
          <rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" xmlns:dcterms="http://purl.org/dc/terms/" xmlns:vCard="http://www.w3.org/2001/vcard-rdf/3.0#" xmlns:vCard4="http://www.w3.org/2006/vcard/ns#" xmlns:bqbiol="http://biomodels.net/biology-qualifiers/" xmlns:bqmodel="http://biomodels.net/model-qualifiers/">
            <rdf:Description rdf:about="#meta_e__GLUT2_GAL">
              <bqbiol:is>
                <rdf:Bag>
                  <rdf:li rdf:resource="https://identifiers.org/SBO:0000284"/>
                </rdf:Bag>
              </bqbiol:is>
            </rdf:Description>
          </rdf:RDF>
        </annotation>
        <listOfReactants>
          <speciesReference species="e__gal" stoichiometry="1" constant="true"/>
        </listOfReactants>
        <listOfProducts>
          <speciesReference species="c__gal" stoichiometry="1" constant="true"/>
        </listOfProducts>
        <kineticLaw>
          <math xmlns="http://www.w3.org/1998/Math/MathML" xmlns:sbml="http://www.sbml.org/sbml/level3/version1/core">
            <apply>
              <divide/>
              <apply>
                <times/>
                <apply>
                  <divide/>
                  <ci> GLUT2_Vmax </ci>
                  <ci> GLUT2_k_gal </ci>
                </apply>
                <apply>
                  <minus/>
                  <ci> e__gal </ci>
                  <apply>
                    <divide/>
                    <ci> c__gal </ci>
                    <ci> GLUT2_keq </ci>
                  </apply>
                </apply>
              </apply>
              <apply>
                <plus/>
                <cn sbml:units="dimensionless" type="integer"> 1 </cn>
                <apply>
                  <divide/>
                  <ci> c__gal </ci>
                  <ci> GLUT2_k_gal </ci>
                </apply>
                <apply>
                  <divide/>
                  <ci> e__gal </ci>
                  <ci> GLUT2_k_gal </ci>
                </apply>
              </apply>
            </apply>
          </math>
        </kineticLaw>
      </reaction>
    </listOfReactions>
