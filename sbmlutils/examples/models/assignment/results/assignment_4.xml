    </listOfInitialAssignments>
    <listOfRules>
      <assignmentRule variable="Cve">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <apply>
            <divide/>
            <ci> Ave </ci>
            <ci> Vve </ci>
          </apply>
        </math>
      </assignmentRule>
      <assignmentRule variable="Vve">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
