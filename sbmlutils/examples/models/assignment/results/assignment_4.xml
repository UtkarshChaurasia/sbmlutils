    <listOfInitialAssignments>
      <initialAssignment symbol="Ave">
        <math xmlns="http://www.w3.org/1998/Math/MathML">
          <ci> IVDOSE </ci>
        </math>
      </initialAssignment>
      <initialAssignment symbol="D">
